{
  "name": "retool-cli",
  "version": "1.0.3",
  "description": "CLI for Retool",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "retool": "./lib/index.js"
  },
  "scripts": {
    "build": "npm run clean && tsc -p . && npm run copy-files && chmod +x lib/index.js",
    "clean": "rimraf lib/",
    "copy-files": "copyfiles -u 1 src/**/*.html lib/",
    "lint": "npx eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tryretool/retool-cli.git"
  },
  "bugs": {
    "url": "https://github.com/tryretool/retool-cli/issues"
  },
  "homepage": "https://github.com/tryretool/retool-cli#readme",
  "dependencies": {
    "connection-string-parser": "^1.0.4",
    "cookie-parser": "^1.4.6",
    "csv-parser": "^3.0.0",
    "express": "^4.18.2",
    "inquirer": "^8.0.0",
    "node-fetch": "^2.6.11",
    "open": "^7.4.2",
    "ora": "^5.4.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.44.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
